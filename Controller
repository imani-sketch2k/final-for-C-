public class StudentWorkerController : Controller
{
    // GET: StudentWorker
    public ActionResult Index()
    {
        StudentWorkerModel studentWorker = new StudentWorkerModel("John Doe", 1, 10, 5);
        ViewBag.Name = studentWorker.Name;
        ViewBag.WeeklySalary = studentWorker.WeeklySalary();
        return View();
    }
}
