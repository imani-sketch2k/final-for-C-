namespace FinalMuganza.Models
{
    public class Student
    {
        public string Name { get; set; }
        public string Id { get; set; }

        public Student(string name, string id)
        {
            Name = name;
            Id = id;
        }

        public override string ToString()
        {
            return $"Student: {Name}, ID: {Id}";
        }
    }
}
