@model MvcApp.Models.StudentWorker

@{
    ViewBag.Title = "Student Worker Details";
}

<h2>Student Worker Details</h2>

<table>
    <tr>
        <td>Name:</td>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <td>ID:</td>
        <td>@Model.ID</td>
    </tr>
    <tr>
        <td>Weekly Salary:</td>
        <td>@Model.WeeklySalary()</td>
    </tr>
</table>
